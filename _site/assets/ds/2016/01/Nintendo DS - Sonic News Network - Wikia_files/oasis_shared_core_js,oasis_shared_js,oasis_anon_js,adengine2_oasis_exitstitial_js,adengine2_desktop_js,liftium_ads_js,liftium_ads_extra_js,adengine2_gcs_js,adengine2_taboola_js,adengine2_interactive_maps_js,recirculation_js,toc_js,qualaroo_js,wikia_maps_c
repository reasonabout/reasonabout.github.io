function onAdLoad(slot,gptEvent,iframe,forcedAdType){var adType=forcedAdType||getAdType(slot.name,gptEvent,iframe),shouldPollForSuccess=false,expectAsyncCollapse=false,expectAsyncHop=false,expectAsyncHopWithSlotName=false,expectAsyncSuccessWithSlotName=false,expectAsyncSuccess=false,successTimer;function noop(){return;}
